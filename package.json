{
  "name": "infinite-scaling",
  "productName": "Infinite scaling",
  "version": "1.0.0",
  "description": "App created by scalers to help other scalers",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "buildInstaller": "electron-builder",
    "generate-docs": "jsdoc --configure .jsdoc.json --verbose"
  },
  "build": {
    "appId": "infinite-scaling",
    "win": {
      "target": "nsis",
      "requestedExecutionLevel": "requireAdministrator"
    },
    "nsis": {
      "uninstallDisplayName": "infinite-scaling",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "keywords": [],
  "author": {
    "name": "Gls Corp",
    "email": "contact@gls-corporation.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "infinite-scaling"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "archiver": "^5.2.0",
    "bootstrap": "^4.5.3",
    "chromium": "^3.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "extract-zip": "^2.0.1",
    "jquery": "^3.5.1",
    "proxy-chain": "^0.4.6",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "better-docs": "^2.3.2",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-reload": "^1.5.0"
  }
}
